<%= will_paginate @messages %>
<ul class='builds'>
  <% for message in @messages %>
    <li>
      <div class='name'><%= message.person.nick %></div> <strong><%= message.text %></strong>
      <div class='right'><%= message.created_at.strftime("%I:%m") %></div>
    </li>
  <% end %>
</ul>
<%= will_paginate @messages %>